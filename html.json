	"list-group-bs4": {
		"scope": "html",
		"prefix": "list-group-bs4",
		"body": [
			"<ul class=\"list-group\">",
			"<li class=\"list-group-item\"></li>",
			"<li class=\"list-group-item\"></li>",
			"</ul>"
		],
		"description": "Basic list-group structure w/ Bootstrap v4.x"
	},
	"table-bs4": {
		"scope": "html",
		"prefix": "table-bs4",
		"body": [
			"<table class=\"table\">",
			"<thead>",
			"<tr>",
			"<th>th1</th>",
			"<th>th2</th>",
			"</tr>",
			"</thead>",
			"<tbody>",
			"<tr>",
			"<td>td1-1</td>",
			"<td>td1-2</td>",
